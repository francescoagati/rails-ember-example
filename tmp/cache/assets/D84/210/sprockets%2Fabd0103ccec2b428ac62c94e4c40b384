(function(exports) {
// ==========================================================================
// Project:  Ember Runtime
// Copyright: б╘2006-2011 Strobe Inc. and contributors.
//            Portions б╘2008-2011 Apple Inc. All rights reserved.
// License:   Licensed under MIT license (see license.js)
